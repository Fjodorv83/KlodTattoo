  <data name="Aftercare_Title" xml:space="preserve">
    <value>Tattoo Care</value>
  </data>
  <data name="Aftercare_Instructions" xml:space="preserve">
    <value>Carefully follow these instructions to ensure perfect healing of your tattoo.</value>
  </data>
  <data name="Aftercare_Step1_Title" xml:space="preserve">
    <value>The Bandage</value>
  </data>
  <data name="Aftercare_Step1_Text" xml:space="preserve">
    <value>Keep the protective bandage on for 2-4 hours after the session.</value>
  </data>
  <data name="Aftercare_Step2_Title" xml:space="preserve">
    <value>Washing</value>
  </data>
  <data name="Aftercare_Step2_Text" xml:space="preserve">
    <value>Gently wash with lukewarm water and neutral soap.</value>
  </data>
  <data name="Aftercare_Step3_Title" xml:space="preserve">
    <value>Drying</value>
  </data>
  <data name="Aftercare_Step3_Text" xml:space="preserve">
    <value>Pat dry with clean paper towel, do not rub.</value>
  </data>
  <data name="Aftercare_Step4_Title" xml:space="preserve">
    <value>Moisturizing</value>
  </data>
  <data name="Aftercare_Step4_Text" xml:space="preserve">
    <value>Apply a thin layer of specific cream 3-4 times a day.</value>
  </data>
  <data name="Aftercare_Step5_Title" xml:space="preserve">
    <value>Protection</value>
  </data>
  <data name="Aftercare_Step5_Text" xml:space="preserve">
    <value>Avoid direct sun, swimming pools, and sea for at least 2 weeks.</value>
  </data>
  <data name="Aftercare_Step6_Title" xml:space="preserve">
    <value>Healing</value>
  </data>
  <data name="Aftercare_Step6_Text" xml:space="preserve">
    <value>Do not scratch the scabs that will form.</value>
  </data>
  <data name="Tips_Title" xml:space="preserve">
