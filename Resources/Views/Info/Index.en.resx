  </data>
  <data name="Tips_Dos_1" xml:space="preserve">
    <value>Drink plenty of water</value>
  </data>
  <data name="Tips_Dos_2" xml:space="preserve">
    <value>Wear comfortable clothes</value>
  </data>
  <data name="Tips_Dos_3" xml:space="preserve">
    <value>Eat well before the session</value>
  </data>
  <data name="Tips_Dos_4" xml:space="preserve">
    <value>Sleep well the night before</value>
  </data>
  <data name="Tips_Dos_5" xml:space="preserve">
    <value>Follow the artist's advice</value>
  </data>
  <data name="Tips_Donts_Title" xml:space="preserve">
    <value>Don'ts</value>
  </data>
  <data name="Tips_Donts_1" xml:space="preserve">
    <value>Alcohol before the session</value>
  </data>
  <data name="Tips_Donts_2" xml:space="preserve">
    <value>Aspirin or blood thinners</value>
  </data>
  <data name="Tips_Donts_3" xml:space="preserve">
    <value>Sun exposure before</value>
  </data>
  <data name="Tips_Donts_4" xml:space="preserve">
    <value>Arriving on an empty stomach</value>
  </data>
  <data name="Tips_Donts_5" xml:space="preserve">
    <value>Bringing too many guests</value>
  </data>
  <data name="Emergency_Title" xml:space="preserve">
    <value>In case of doubt</value>
  </data>
  <data name="Emergency_Text" xml:space="preserve">
    <value>If you notice excessive redness, abnormal swelling, or have doubts about healing, contact us immediately.</value>
  </data>
